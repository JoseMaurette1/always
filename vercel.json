{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_SUPABASE_URL": "@next_public_supabase_url",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": "@next_public_supabase_anon_key"
  },
  "buildCommand": "NEXT_DISABLE_SOURCEMAPS=1 NEXT_DISABLE_FETCH_CACHE=1 NEXT_DISABLE_PAGE_DATA_COLLECTION=1 next build --no-lint"
}
